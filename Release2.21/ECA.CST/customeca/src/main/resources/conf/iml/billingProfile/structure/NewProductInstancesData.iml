<iml:definitions
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:Convergys-Interface-Markup-Language
  http://icetc.netcracker.com/development/iml.xsd"
  xmlns="urn:Convergys-Interface-Markup-Language"
  xmlns:iml="urn:Convergys-Interface-Markup-Language"
  defaultComponentId="CUSTOMECA"
  defaultServiceId="BillingProfile">

<structureDef name="NewProductInstancesData" >
    <doc>Array of information regarding the new products to be added (or product instance to be created)  </doc>
   		
	    
	    <field name="ProductId">
        	<doc>ID of the product  .</doc>
			<int32 nullable="false" maxDigits="9"/>
    	</field>
   		<field name="ProductStartDtm">
	        <doc>Date and Time when the product is taken.</doc>
			<dateTime includeTimeZone="false" nullable="false"/>
   		</field>
   		<field name="ProductEndDtm">
	        <doc>Date and time when the customer stops taking the product.</doc>
			<dateTime includeTimeZone="false" nullable="true"/>
   		</field>
   		 <field name="ParentProductId">
        	<doc>ID of the parent product.</doc>
			<int32 nullable="false" maxDigits="9"/>
    	</field>
    	 <field name="BillingTariffId">
        	<doc>RB Price plan ID under which this customer-product will be billed.</doc>
			<int32 nullable="false" maxDigits="9"/>
    	</field>
    	
    	<field name="NewProductInstanceAttributesArray">
	        <doc>Array to specify products custom attributes values</doc>
	        <array nullable="false">
				<structure name="NewProductInstanceAttributesData" nullable="false"/>
			</array>	
   		</field> 
    	
    	<field name="EventSourceArray">
	        <doc>Array to specify the source for the event generating product</doc>
	        <array nullable="true">
				<structure name="EventSource" nullable="false"/>
			</array>	
   		</field> 
    	
 </structureDef>
</iml:definitions>