<iml:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:Convergys-Interface-Markup-Language
  http://icetc.netcracker.com/development/iml.xsd"
	xmlns="urn:Convergys-Interface-Markup-Language" xmlns:iml="urn:Convergys-Interface-Markup-Language"
	defaultComponentId="CUSTOMECA" defaultServiceId="BillingProfile">

	<structureDef name="ModifyEventSourceData">
		<doc>Array of information for ModifyEventSourceData  </doc>

		<field name="ProductId">
			<doc>ID of the product  .</doc>
			<int32 nullable="false" maxDigits="9" />
		</field>
		<field name="Action">
	        <doc>
				Action - valid values -  Add/Remove/Modify/Terminate.					
			</doc>
			<string nullable="false" maxLength="20"/>
   		</field>
		
		<field name="TOMsServiceInstancekey">
			<doc>TOMsServiceInstancekey </doc>
			<string nullable="false" maxLength="40" />
		</field>

		<field name="EventSource">
			<doc>Gets the event source name, for example, the telephone number. </doc>
			<string nullable="false" maxLength="40" />
		</field>
		
		<field name="EventTypeId">
			<doc>Gets the event type ID. The type of event generated by this event source. </doc>
			<int32 nullable="false" maxDigits="9" />
		</field>

		<field name="RatingTariffId">
			<doc>RatingTariffId .</doc>
			<int32 nullable="false" maxDigits="9" />
		</field>

		<field name="StartDtm">
			<doc>Date and Time when the event source becomes active.</doc>
			<dateTime includeTimeZone="false" nullable="false" />
		</field>
		<field name="EndDtm">
			<doc>End Date.The date when the event source ceases to be active.</doc>
			<dateTime includeTimeZone="false" nullable="true" />
		</field>

		<field name="EventSourceLabel">
			<doc>Gets the event source label. A label that appears on bills for the eventsource. </doc>
			<string nullable="false" maxLength="40" />
		</field>

		<field name="EventSourceText">
			<doc>Gets the event source text. Additional text related to this event source.</doc>
			<string nullable="true" maxLength="255" />
		</field>
	</structureDef>
</iml:definitions>