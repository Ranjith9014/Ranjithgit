<iml:definitions
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:Convergys-Interface-Markup-Language
  http://icetc.netcracker.com/development/iml.xsd"
  xmlns="urn:Convergys-Interface-Markup-Language"
  xmlns:iml="urn:Convergys-Interface-Markup-Language"
  defaultComponentId="CUSTOMECA"
  defaultServiceId="BillingProfile">

<structureDef name="NewAccountPaymentData" >
    <doc>The structure contains required input data for creating account payment  </doc>
   		
	    
	    <field name="Paymentdate">
        	<doc>The nominal date of the account transaction</doc>
			<dateTime includeTimeZone="false" nullable="true"/>
    	</field>
		
		<field name="PaymentMny">
	        <doc>The amount of money involved in the account transaction.</doc>
			<int64 nullable="false" maxDigits="18"/>
   		</field>
		
		<field name="PaymentTxt">
	        <doc>Optional text input when an account/physical payment is created</doc>
			<string nullable="true" maxLength="255" />
   		</field>
		
		<field name="PaymentRef">
	        <doc>A flexible field for recording information against an account/physical payment</doc>
			<string nullable="true" maxLength="40" />
		</field>
		
		<field name="PhysicalCustomerRef">
	        <doc>Physical Customer to which the payment is being created</doc>
			<string nullable="false" maxLength="20" />
		</field>
		
		<field name="CreatedDtm">
	        <doc>The date/time that the account payment entered into the system</doc>
			<dateTime includeTimeZone="false" nullable="true"/>
			
   		</field>
		
		<field name="PaymentMethodId">
	        <doc>Allows a physical payment record to be associated with a particular batch of payments</doc>
			<int32 nullable="false" maxDigits="9"/>
		</field>
		
		<field name="RefundBoo">
	        <doc>If payment money is negative then the parameter refund boo in NewAccountPaymentData 
					indicates whether it is refund payment. When payment money is positive, refund boo is optional 
						but if provided then it must be false (F)</doc>
			<boolean nullable="true"/>
		</field>
		
    	
 </structureDef>
</iml:definitions>