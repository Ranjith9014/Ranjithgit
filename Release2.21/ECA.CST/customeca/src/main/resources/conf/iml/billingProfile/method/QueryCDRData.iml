<iml:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:Convergys-Interface-Markup-Language
  http://icetc.netcracker.com/development/iml.xsd"
	xmlns="urn:Convergys-Interface-Markup-Language" xmlns:iml="urn:Convergys-Interface-Markup-Language"
	defaultComponentId="CUSTOMECA" defaultServiceId="BillingProfile">
	<methodDef name="QueryCDRData">
		<internal name="CvgMethodType" value="write" />
		<doc>

		</doc>

		<input name="IntegratorContext">
			<doc>
				Integration context for caller specific information.
			</doc>
			<structure nullable="false" name="IntegratorContext" />
		</input>

		<input name="AccountNum">
			<doc>AccountNum</doc>
			<string nullable="false" maxLength="20" />
		</input>

		<input name="EventSource">
			<doc>Event source name e.g. MSISDN.</doc>
			<string nullable="false" maxLength="40" />
		</input>

		<input name="EventTypeID">
			<doc>Specify the type of event generated by the event source .</doc>
			<int32 nullable="false" maxDigits="9" />
		</input>


		<input name="AppliedBill">
			<doc>   
				AppliedBill.      
		  	</doc>
			<see name="AppliedBill" />
			<enum nullable="true" name="AppliedBill" />
		</input>

		<input name="OptionalInputString">
			<doc>OptionalInputString</doc>
			<string nullable="true" maxLength="40" />
		</input>


		<input name="FilterArray">
			<doc>Array to specify products custom attributes values</doc>
			<array nullable="true">
				<structure name="FilterArrayElements" nullable="true" />
			</array>
		</input>
		
		<input name="SortingArray">
			<doc>Array to specify products custom attributes values</doc>
			<array nullable="true">
				<structure name="SortingArrayElements" nullable="true" />
			</array>
		</input>
		
		<input name="CDRDetailsKey">
			<doc>The Key Object obtained from the level3 drill down.It contains details tag that is provided in the queryLineItemDetails API.</doc>
			<structure nullable="true" name="CDRDetailsKey" />
		</input>


		<input name="Pagination">
			<doc>account primary key information</doc>
			<structure nullable="false" name="Pagination" />
		</input>

		<output name="QueryCDROutputs">
			<doc>Array of the QueryCDROutputs data</doc>
			<structure nullable="true" name="QueryCDROutputs" />
		</output>


		<exception name="ParameterException" componentId="CommonIML"
			serviceId="">
			<doc>This is thrown on data validation error.</doc>
		</exception>
		<exception name="NullParameterException" componentId="CommonIML"
			serviceId="">
			<doc>This is thrown on data validation error.</doc>
		</exception>
		<exception name="ApplicationException" componentId="Platform"
			serviceId="">
			<doc>This is thrown on data validation error.</doc>
		</exception>



	</methodDef>
</iml:definitions>