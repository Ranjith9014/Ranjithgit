<iml:definitions
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:Convergys-Interface-Markup-Language
  ../../../../iml.xsd"
  xmlns="urn:Convergys-Interface-Markup-Language"
  xmlns:iml="urn:Convergys-Interface-Markup-Language"

  defaultComponentId="ECA"
  defaultServiceId="Config"
>
  <structureDef name="CPSSummary" version="5.1">
    <doc>
The summary bean for the contracted point of supply data.
    </doc>

    <extends serviceId="" name="DataObject"/>

    <field name="CPSId">
      <internal name="ECA_TableColumn" value="PVCONTRACTEDPOINTOFSUPPLY2.CPS_ID NUMBER(9) NOT NULLABLE"/>
      <doc>
Gets the identifier for the contracted point of supply. 
      </doc>
      <int32 nullable="false" maxDigits="9"/>
    </field>

    <field name="CPSName">
      <internal name="ECA_TableColumn" value="PVCONTRACTEDPOINTOFSUPPLY2.CPS_NAME VARCHAR2(40) NOT NULLABLE"/>
      <doc>
Gets the name of the contracted point of supply.
      </doc>
      <string nullable="false" maxLength="255"/>
    </field>
  </structureDef>
</iml:definitions>

<!-- eof ECA/Config/structure/5.1/CPSSummary.iml -->
