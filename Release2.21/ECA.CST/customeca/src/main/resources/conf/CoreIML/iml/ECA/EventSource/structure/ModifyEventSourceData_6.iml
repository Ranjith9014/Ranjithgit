<iml:definitions
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:Convergys-Interface-Markup-Language
  ../../../iml.xsd"
  xmlns="urn:Convergys-Interface-Markup-Language"
  xmlns:iml="urn:Convergys-Interface-Markup-Language"

  defaultComponentId="ECA"
  defaultServiceId="EventSource"
>
  <structureDef name="ModifyEventSourceData" version="6" since="3.1">
    <doc>
The modify event source data bean.
    </doc>

    <extends serviceId="" name="DataObject"/>

    <mixin serviceId="" name="HistoriedByDateTime"/>

    <field name="EventSource">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.EVENT_SOURCE VARCHAR2(40) NOT NULLABLE"/>
      <doc>
Gets the event source name, for example, the telephone number. 
      </doc>
      <string nullable="false" maxLength="40"/>
    </field>

    <field name="EventTypeId">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.EVENT_TYPE_ID NUMBER(9) NOT NULLABLE"/>
      <doc>
Gets the event type ID. The type of event generated by this event source. 
      </doc>
      <int32 nullable="false" maxDigits="9"/>
    </field>

    <field name="StartDtm" mixin="true">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.START_DTM DATE NOT NULLABLE"/>
      <doc>
Gets the start date. The date when the event source becomes active.
      </doc>
      <dateTime includeTimeZone="false" nullable="false"/>
    </field>

    <field name="EndDtm" mixin="true">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.END_DTM DATE NULLABLE"/>
      <doc>
Gets the end date. The date when the event source ceases to be active.
      </doc>
      <dateTime includeTimeZone="false" nullable="true"/>
    </field>

    <field name="EventSourceLabel">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.EVENT_SOURCE_LABEL VARCHAR2(40) NOT NULLABLE"/>
      <doc>
Gets the event source label. A label that appears on bills for the event
source. 
      </doc>
      <string nullable="false" maxLength="40"/>
    </field>

    <field name="EventSourceText">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.EVENT_SOURCE_TXT VARCHAR2(255) NULLABLE"/>
      <doc>
Gets the event source text. Additional text related to this event source.
      </doc>
      <string nullable="true" maxLength="255"/>
    </field>

    <field name="CreditLimitMny">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.CREDIT_LIMIT_MNY NUMBER(18) NULLABLE"/>
      <doc>
Gets the credit limit for this event source. 
      </doc>
      <int64 nullable="true" maxDigits="18"/>
    </field>

    <field name="RatingTariffPK">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.RATING_TARIFF_ID NUMBER(9) NULLABLE"/>
      <doc>
Gets the rate plan ID. The rate  plan used to rate the event's
source/event type combination. 
      </doc>
      <see serviceId="RatingTariff" name="RatingTariffPK"/>
      <structure nullable="true" serviceId="RatingTariff" name="RatingTariffPK"/>
    </field>

    <field name="CompetitorRatingTariffPK">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.COMPETITOR_RATING_TARIFF_ID NUMBER(9) NULLABLE"/>
      <doc>
Gets the competitor rate plan ID. This is the rate price plan used to rate
the event's source/event type combination.
      </doc>
      <see serviceId="RatingTariff" name="RatingTariffPK"/>
      <structure nullable="true" serviceId="RatingTariff" name="RatingTariffPK"/>
    </field>

    <field name="CopyGuidingRules">
      <doc>
Specifies whether event guiding rules should be copied from an existing event
source to the new event source. Valid values are true and false. It
may be null,  in which case it will take the default value (which is false). 
If set to true,
the guiding rules will be copied from an event source which ended one second
before the new event source has been created.
      </doc>
      <boolean nullable="true"/>
    </field>

    <field name="EventDeletionMode">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.EVENT_DELETION_MODE NUMBER(1) NULLABLE"/>
      <doc>
Determines whether or not to delete the event records immediately after billing without
archiving.
      </doc>
      <enum nullable="true" serviceId="Event" name="EventDeletionMode"/>
    </field>
 
    <field name="MaskBillRuleId">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.MASK_BILL_RULE_ID NUMBER(9) NULLABLE"/>
      <doc>
The masking rule Id used to mask this event on the bill.
      </doc>
      <int32 nullable="true" maxDigits="9"/>
    </field>

    <field name="MaskStoreRuleId">
      <internal name="ECA_TableColumn" value="PVCUSTEVENTSOURCE6.MASK_STORE_RULE_ID NUMBER(9) NULLABLE"/>
      <doc>
The masking rule Id used to mask this event in the datastore.
      </doc>
      <int32 nullable="true" maxDigits="9"/>
    </field>

  </structureDef>

</iml:definitions>

<!-- eof ECA/ProductInstance/structure/ModifyEventSourceData_6.iml -->
