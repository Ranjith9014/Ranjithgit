  <exceptionDef name="DataPartitioningException">
    <doc>
This exception is thrown when a user attempts to access data for a customer
whom he is not allowed access to by the data partitioning rules.
    </doc>
    <extends name="BusinessLogicException"/>
  </exceptionDef>
</iml:definitions>

