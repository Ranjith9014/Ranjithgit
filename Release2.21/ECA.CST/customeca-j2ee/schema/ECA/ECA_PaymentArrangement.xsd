          <xsd:element name="dueDat" type="xsd:dateTime"/>
          <xsd:element name="createdBillSeq" type="xsd:int"/>
       </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AccountPaymentArrangementData_1">
    <xsd:complexContent>
      <xsd:extension base="eca:DataObject">
        <xsd:sequence>
          <xsd:element name="paymentArrangementId" type="xsd:int"/>
          <xsd:element name="validFromDat" type="xsd:dateTime"/>
          <xsd:element name="validToDat" type="xsd:dateTime"/>
          <xsd:element name="accountPaymentArrangementStatus" type="ecaPaymentArrangement:AccountPaymentArrangementStatus"/>
          <xsd:element name="billSeq" type="xsd:int"/>
          <xsd:element name="billVersion" type="xsd:int"/>
          <xsd:element name="outstandingTot" type="xsd:double"/>
          <xsd:element name="paymentArrangementType" type="ecaConfig:PaymentArrangementType"/>
          <xsd:element name="paymentArrangementLevel" type="ecaPaymentArrangement:PaymentArrangementLevel"/>
          <xsd:element name="numBilledInstallments" type="xsd:int"/>
