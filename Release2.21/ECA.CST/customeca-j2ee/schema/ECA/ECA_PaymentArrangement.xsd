       1 = Voluntary

-->
    </xsd:restriction>
  </xsd:simpleType>


  <!-- Structure and Exception Types -->
  <xsd:complexType name="AccountBillingScheduleData_1">
    <xsd:complexContent>
      <xsd:extension base="eca:DataObject">
        <xsd:sequence>
          <xsd:element name="dueDat" type="xsd:dateTime"/>
          <xsd:element name="createdBillSeq" type="xsd:int"/>
          <xsd:element name="installmentMny" type="xsd:long"/>
          <xsd:element name="installmentStatus" type="ecaPaymentArrangement:InstallmentStatus"/>
          <xsd:element name="installmentNum" type="xsd:int"/>
          <xsd:element name="scheduledBillSeq" type="xsd:int" minOccurs="0" nillable="true"/>
          <xsd:element name="billCreatedAccPaySeq" type="xsd:int" minOccurs="0" nillable="true"/>
          <xsd:element name="hint" type="xsd:int" minOccurs="0" nillable="true"/>
          <xsd:element name="accountNum" type="xsd:string"/>
          <xsd:element name="paymentArrangementSeq" type="xsd:int"/>
       </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AccountBillingSchedulePK">
    <xsd:complexContent>
      <xsd:extension base="eca:PrimaryKey">
        <xsd:sequence>
          <xsd:element name="accountNum" type="xsd:string"/>
