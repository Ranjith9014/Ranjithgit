       </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="QueryAccountPaymentArrangementOutput_1">
        <xsd:sequence>
          <xsd:element name="queryAccountPaymentArrangementResult" type="ecaPaymentArrangement:QueryAccountPaymentArrangementResult_1"/>
       </xsd:sequence>
  </xsd:complexType>

