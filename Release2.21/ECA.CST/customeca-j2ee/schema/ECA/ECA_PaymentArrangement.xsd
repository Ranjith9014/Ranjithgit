          <xsd:element name="installmentStartDat" type="xsd:dateTime" minOccurs="0" nillable="true"/>
          <xsd:element name="terminationDat" type="xsd:dateTime" minOccurs="0" nillable="true"/>
          <xsd:element name="paTermReasonId" type="xsd:int" minOccurs="0" nillable="true"/>
          <xsd:element name="hint" type="xsd:int" minOccurs="0" nillable="true"/>
          <xsd:element name="accountNum" type="xsd:string"/>
          <xsd:element name="paymentArrangementSeq" type="xsd:int"/>
       </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AccountPaymentArrangementPK">
    <xsd:complexContent>
      <xsd:extension base="eca:PrimaryKey">
        <xsd:sequence>
          <xsd:element name="accountNum" type="xsd:string"/>
