          <xsd:element name="accountReceivableClasses" type="ecaPaymentArrangement:ArrayOfAccountReceivableClassData_1" minOccurs="0" nillable="true"/>
          <xsd:element name="truncated" type="xsd:boolean"/>
       </xsd:sequence>
      </xsd:extension>
