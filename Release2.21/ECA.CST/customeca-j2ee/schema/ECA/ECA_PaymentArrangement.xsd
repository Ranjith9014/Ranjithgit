  <!-- Enumeration Types -->
  <xsd:simpleType name="AccountPaymentArrangementStatus">
    <xsd:restriction base="xsd:int">
<!-- Enums are represented as int values in IML. The following list provides a mapping of
the different enum options supported by the server. Please note that the server might have
been upgraded to a more uptodate release and might support additional options. Some options
may also be deprecated.

       0 = ACTIVE
       1 = COMPLETED
       2 = EXPIRED
       3 = EXPIREDANDCOMPLETED

-->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="InstallmentStatus">
    <xsd:restriction base="xsd:int">
<!-- Enums are represented as int values in IML. The following list provides a mapping of
the different enum options supported by the server. Please note that the server might have
been upgraded to a more uptodate release and might support additional options. Some options
may also be deprecated.

       0 = CREATED
       1 = BILLED
       2 = EXPIRED
       3 = EXPIREDBILLED

-->
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PaymentArrangementLevel">
    <xsd:restriction base="xsd:int">
