          <xsd:element name="paymentArrangementTypeSet" type="ecaConfig:ArrayOfPaymentArrangementType" minOccurs="0" nillable="true"/>
          <xsd:element name="paymentArrangementLevelSet" type="ecaPaymentArrangement:ArrayOfPaymentArrangementLevel" minOccurs="0" nillable="true"/>
          <xsd:element name="lastAccountPaymentArrangementPK" type="ecaPaymentArrangement:AccountPaymentArrangementPK" minOccurs="0" nillable="true"/>
